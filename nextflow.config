process {
    scratch = true
    process.conda = "$baseDir/custom-conda-env.yml"
    withLabel: parallel_job {
        executor = 'pbspro'
        clusterOptions = '-A st-tdjc-1'
        cpus = 4
        memory = 8.GB
        time = '1h'
    }
    withLabel: local_job {
        executor = 'local'
    }
}
